{
    "_metadata": {
        "major_version": 1,
        "minor_version": 1
    },
    "display_information": {
        "name": "Horodateur"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": true,
            "messages_tab_enabled": false,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "Horodateur",
            "always_online": false
        },
        "shortcuts": [
            {
                "name": "Run sample shortcut",
                "type": "global",
                "callback_id": "sample_shortcut_id",
                "description": "Runs a sample shortcut"
            }
        ],
        "slash_commands": [
            {
                "command": "/infos",
                "description": "information sur le bot Horodatage",
                "should_escape": false
            },
            {
                "command": "/list_users",
                "description": "Liste les utilisateurs qui sont entregister pour l'horodatage",
                "should_escape": false
            },
            {
                "command": "/add_user",
                "description": "Ajoute un utilisateur Ã  l'horodatage",
                "should_escape": false
            },
            {
                "command": "/test",
                "description": "Commande de test du developpeur",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:history",
                "app_mentions:read",
                "chat:write",
                "commands"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_home_opened",
                "message.channels",
                "app_mention"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}